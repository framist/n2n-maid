{
  "$schema": "https://tauri.app/schemas/config.schema.json",
  "productName": "N2N Maid",
  "version": "0.1.0",
  "identifier": "com.n2n.maid",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://127.0.0.1:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "N2N Maid · 恩兔酱",
        "width": 420,
        "height": 680,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    },
    "trayIcon": {
      "id": "main",
      "iconPath": "../icons/icon.png",
      "iconAsTemplate": true,
      "menuOnLeftClick": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../icons/32x32.png",
      "../icons/128x128.png",
      "../icons/128x128@2x.png",
      "../icons/icon.icns",
      "../icons/icon.ico"
    ],
    "resources": ["../bin/*"],
    "externalBin": [],
    "copyright": "",
    "category": "Network",
    "shortDescription": "N2N VPN Client UI",
    "longDescription": "A modern, lightweight GUI for N2N P2P VPN",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "plugins": {}
}
